{
  "presets": [
    [
      "@babel/env",
      {
        "modules": "false",
        "shippedProposals": true,
        "useBuiltIns": "usage",
        "corejs": "3",
        "targets": {
          "browsers": ["last 2 version", "> 5%", "Safari 7"]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/proposal-class-properties", { "loose": true }]
  ]
}
